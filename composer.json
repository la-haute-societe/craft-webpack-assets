{
    "name": "la-haute-societe/craft-webpack-assets",
    "description": "Webpack assets for Craft CMS",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "webpackassets"
    ],
    "support": {
        "docs": "https://github.com/la-haute-societe/craft-webpack-assets/blob/master/README.md",
        "issues": "https://github.com/la-haute-societe/craft-webpack-assets/webpack-assets/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "La Haute Société",
            "homepage": "https://www.lahautesociete.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "lhs\\webpackassets\\": "src/"
        }
    },
    "extra": {
        "name": "WebpackAssets",
        "handle": "webpack-assets",
        "schemaVersion": "2.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/la-haute-societe/craft-webpack-assets/webpack-assets/master/CHANGELOG.md",
        "documentationUrl": "https://github.com/la-haute-societe/craft-webpack-assets/blob/master/README.md",
        "components": {
            "jsonReader": "lhs\\webpackassets\\services\\JsonReader"
        },
        "class": "lhs\\webpackassets\\WebpackAssets"
    }
}
